    {
        dir *= -1;
    }
    
}   
y += vsp;

//Enemy collision
if (place_meeting(x,y,obj_player))
{
    if (obj_player.y &lt; y-16)
    {
        with (obj_player) vsp =-jumpspeed;
        hp -= 25;;
    }
    else
    {
        game_restart();
    }
}
//death
if (hp&lt;=0){
    instance_destroy();
}

image_xscale = dir;




